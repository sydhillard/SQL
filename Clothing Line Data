SELECT * FROM workout_clothes ORDER BY price;
SELECT * FROM workout_clothes ORDER BY quantity;
SELECT * FROM workout_clothes ORDER BY color;
SELECT SUM(price) FROM workout_clothes;
SLECT SUM(quantity) FROM workout_clothes;
